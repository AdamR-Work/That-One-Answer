{{#if hbsObj.category}}
<ol class="category-list">   
    {{#each hbsObj.category}}
    <li class="card w-50">
        <div class="card-body">
            <a href="/category/{{dataValues.id}}">
            <h5 class="card-title">{{dataValues.title}}</h5>
            </a>
            <p class="card-text">{{dataValues.description}}</p>
        </div>
    </li>
    {{/each}}

</ol>
{{else}}
<p>Hello world!</p>
{{/if}}